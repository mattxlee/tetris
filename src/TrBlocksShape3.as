package  {		public class TrBlocksShape3 extends TrAbstractBlocks {		public function TrBlocksShape3(gameData: ITrGameData) {						setGameData(gameData);			// constructor code						var shape1:Array =				[[0, 0, 6],				[6, 6, 6]];						  			var shape2:Array =				[[6, 0],				[6, 0],				[6, 6]];						  			var shape3:Array =				[[6, 6, 6],				[6, 0, 0]];						  			var shape4:Array =				[[6, 6],				[0, 6],				[0, 6]];						  			addShape(shape1);			addShape(shape2);			addShape(shape3);			addShape(shape4);					}	}	}